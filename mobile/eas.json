{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "production": {
      "distribution": "store",
      "autoIncrement": true,
      "env": {
        "EAS_BUILD_DISABLE_MAVEN_CACHE": "1"
      },
      "android": {
        "autoIncrement": true,
        "image": "latest"
      },
      "ios": {
        "autoIncrement": true
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "autoIncrement": true,
        "image": "latest"
      },
      "ios": {
        "autoIncrement": true
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "autoIncrement": true
      },
      "ios": {
        "autoIncrement": true
      }
    }
  },
  "submit": {
    "production": {}
  }
}
