{
  "name": "guyana-booker-mobile",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "version:bump": "node ./scripts/bump-version.mjs --patch",
    "version:bump:minor": "node ./scripts/bump-version.mjs --minor",
    "version:bump:major": "node ./scripts/bump-version.mjs --major",
    "version:set": "node ./scripts/bump-version.mjs --set",
    "ios:release": "npm run version:bump && eas build -p ios --profile production",
    "android:release": "npm run version:bump && eas build -p android --profile production"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "axios": "^1.13.2",
    "babel-preset-expo": "~54.0.0",
    "expo": "^54.0.24",
    "expo-build-properties": "~1.0.10",
    "expo-constants": "~18.0.12",
    "expo-image-picker": "~17.0.8",
    "expo-linking": "~8.0.11",
    "expo-location": "~19.0.7",
    "expo-secure-store": "~15.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-calendars": "^1.1314.0",
    "react-native-dotenv": "^3.4.11",
    "react-native-fbsdk-next": "^13.4.3",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-maps": "1.20.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "sentry-expo": "^7.1.1",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-community/cli": "^14.0.1",
    "@types/react": "~19.1.10",
    "typescript": "~5.9.2"
  },
  "private": true,
  "overrides": {
    "react": "19.1.0",
    "react-dom": "19.1.0"
  }
}
