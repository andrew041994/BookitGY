{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://bookitgy.onrender.com/:path*"
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "header",
          "key": "accept",
          "value": "text/html"
        }
      ],
      "destination": "/index.html"
    }
  ]
}
